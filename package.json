{
  "name": "nicecode",
  "version": "0.2.1",
  "authors": [
    "dev<710328466@qq.com>"
  ],
  "exports": {
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "father-build",
    "release": "npm run build && npm publish",
    "start": "dumi dev",
    "docs:build": "NODE_ENV=production dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:update": "umi-test --updateSnapshot",
    "test:coverage": "umi-test --coverage",
    "pub": "pnpm changeset publish",
    "v": "pnpm changeset version",
    "cz": "git add . && git cz",
    "log": "conventional-changelog -n ./packages/changelog/src -i changelog.md -s -r 0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./packages/commit"
    }
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@nicecode/commit-lint": "^0.0.9",
    "@nicecode/mofish": "^3.37.1",
    "@types/jest": "^25.1.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^18.0.0",
    "@umijs/test": "^3.3.4",
    "commitizen": "^4.2.4",
    "commitlint": "^12.1.4",
    "conventional-changelog-cli": "^2.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "father-build": "^1.22.1",
    "gh-pages": "^3.0.0",
    "react-test-renderer": "^17.0.2",
    "simple-git-hooks": "^2.8.1",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@umijs/lint": "^4.0.0",
    "babel-plugin-import": "^1.13.8",
    "dumi": "^2.2.13",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "private": true,
  "license": "MIT"
}
